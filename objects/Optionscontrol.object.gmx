            <string>///Menu control
if keyboard_check_pressed(global.key_meelee)
{
    if selection=1 global.soundvolume=1
    if selection=2 global.musicvolume=0.8
    //if selection=3 room_goto(Keyconfig)
    if selection=4 room_goto(Menu)
}
if keyboard_check_pressed(global.key_right)
{
    if selection=1 if global.soundvolume&lt;1 global.soundvolume+=0.1
    if selection=2 {if global.musicvolume&lt;1 global.musicvolume+=0.1 FMODInstanceSetVolume(Initializer.musicnameI,global.musicvolume)}
}
if keyboard_check_pressed(global.key_left)
{
    if selection=1 if global.soundvolume&gt;0 global.soundvolume+=-0.1
    if selection=2 {if global.musicvolume&gt;0 global.musicvolume+=-0.1 FMODInstanceSetVolume(Initializer.musicnameI,global.musicvolume)}
}
if keyboard_check_pressed(global.key_down) if selection&lt;4 selection += 1 else selection = 1
if keyboard_check_pressed(global.key_jump) if selection&gt;1 selection -= 1 else selection = 4
</string>
